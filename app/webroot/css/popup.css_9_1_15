.spacer {
	clear:both;
}

.pop-outer {
	overflow:hidden;
}
.pop-up-hdng {
	font-family:Arial, Helvetica, sans-serif;
	font-size:16px;
	color:#333;
	border-bottom:1px dashed #e3e3e3;
	line-height:20px;
	margin:5px 20px 15px;
	padding-bottom:5px;
	line-height:30px;
}
.row {
	margin:0;
}
.col-sm-12 {
	width:100%;
	position: relative;
	min-height: 1px;
	padding-right: 15px;
}
.col-sm-1 {
	float:left;
}
.col-sm-6 {
	width:50%;
	position: relative;
	min-height: 1px;
	float:left;
}
.form-group {
	float:left;
	clear:both;
	z-index:1;
	margin-bottom:4px;
	position:relative;
	width:100%;
}
.form-group.imp {
	z-index:999;
}
.form-group label {
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
	float: left;
	padding-right: 15px;
	text-align: right;
	color: #888;
	width:32%;
	line-height:30px;
}
.form-group label.bgr {
	width:35%;
}
.colon {
	float: left;
	margin: 5px;
	padding: 1px;
}
.req:after {
    color: #cc0000;
    content: "*";
    font-size: 14px;
    padding-left: 4px;
}
.parsley-error-list li {
    color: #cc0000;
    font-size: 12px;
    list-style: outside none none;
}
/* validation */
	input.parsley-error,textarea.parsley-error,select.parsley-error,.parsley-error .ms-selection .ms-list,.parsley-error .select2-container-multi .select2-choices,.parsley-error .input_validate {border:1px solid #cc0000 !important;background:#fff2f2 !important}
	.parsley-error-list {margin:0;padding:5px 0 0}
	.parsley-error-list li {list-style:none;font-size:12px;color:#cc0000}
	div.parsley-error .parsley-error-list {margin-top:5px}
	.parsley-error-list + label {margin-top:10px}
.form-group .col-sm-10 {
	width:60%;
	float:left;
	padding-left:15px;
}
.form-group .col-sm-10 input, .form-group .col-sm-10 select{
	height: 30px;
	padding: 5px 8px;
	font-size: 13px;
	line-height: 1.4;
	background-color: #fff;
	border: 1px solid #d9d9d9;
	border-top-color: #c0c0c0;
	border-radius: 1px;
	-webkit-box-shadow: none;
	box-shadow: none;
	-webkit-transition: none;
	transition: none;
	width:100%;
}
.form-group.slt-sm .col-sm-10 select {
	width:46%;
	margin-right:4%;
	float:left;
}
.form-group.slt-sm .col-sm-10 input {
	width:50%;
}
.btn {
	display: inline-block;
	margin-bottom: 0;
	font-weight: 400;
	text-align: center;
	vertical-align: middle;
	cursor: pointer;
	background-image: none;
	border: 1px solid transparent;
	white-space: nowrap;
	font-size: 14px;
	border-radius: 4px;
	min-width:80px;
	height:32px;
	margin-right:10px;
	margin-top:10px;
}
.success {
	color: #fff;
	text-shadow: 0 1px rgba(0,0,0,0.1);
	background-image: -webkit-gradient(linear, left 0, left 100%, from(#35aa47), to(#35aa47));
	background-image: -webkit-linear-gradient(top, #35aa47, 0%, #35aa47, 100%);
	background-image: -moz-linear-gradient(top, #35aa47 0, #35aa47 100%);
	background-image: linear-gradient(to bottom, #35aa47 0, #35aa47 100%);
	background-repeat: repeat-x;
	border: 1px solid #359947;
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff35aa47', endColorstr='#ff35aa47', GradientType=0);
	filter:progid:DXImageTransform.Microsoft.gradient(enabled=false)
}
